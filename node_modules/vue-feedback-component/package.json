{
  "name": "vue-feedback-component",
  "version": "1.7.2",
  "main": "dist/feed-back.umd.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "start": "npm run serve",
    "serve": "PKG_TYPE=component vue-cli-service serve",
    "lib": "PKG_TYPE=npm vue-cli-service build --target lib --inline-vue --mode production --name feed-back src/index.js",
    "build": "PKG_TYPE=component vue-cli-service build --target wc --inline-vue --name feed-back src/App.vue"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
